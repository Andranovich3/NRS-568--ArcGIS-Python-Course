list_a = ['dog', 'cat', 'rabbit', 'hamster', 'gerbil']
list_b = ['dog', 'hamster', 'snake']
newList = []

#To check for overlapping
for i in list_a:
    for z in list_b:
        if i == z:
            newList.append(i)
print newList

#To check for items that don't overlap

for i in list_a:
    if not i in list_b:
        newList.append(i)

for z in list_b:
    if not z in list_a:
        newList.append(z)

print newList

